{
  "openapi": "3.1.0",
  "info": {
    "title": "Seoul Visitor Proxy API",
    "description": "서울시 유동인구 데이터를 JSON으로 반환하는 프록시 API입니다.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://seoul-visitor-proxy.vercel.app/api/visitor-count"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "operationId": "getVisitorCounts",
        "summary": "서울시 유동인구 센서 데이터 조회",
        "parameters": [
          {
            "name": "apiKey",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "default": "7146505663676f6f3833484d725366"
            }
          },
          {
            "name": "startIndex",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "example": 1
            }
          },
          {
            "name": "endIndex",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "example": 5
            }
          }
        ],
        "responses": {
          "200": {
            "description": "유동인구 JSON 데이터",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    }
  }
}
